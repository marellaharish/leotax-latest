{
  "name": "GYM Manager App",
  "version": "1.0.0",
  "description": "The application is about managing and automating some manual processes",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "build": "tsc && node backend/fix-imports.js",
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "app": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/feedGYM",
    "data:destroy": "node backend/feedGYM -d"
  },
  "author": "Arya",
