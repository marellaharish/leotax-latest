    "typescript": "^5.5.3"
  },
  "overrides": {
    "chokidar": "3.5.3",
    "glob-parent": "6.0.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.717.0",
    "@babel/core": "^7.22.10",
    "@coreui/react-chartjs": "^2.1.3",
    "@react-oauth/google": "^0.11.1",
    "autoprefixer": "^10.4.15",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.1",
    "colors": "^1.4.0",
    "config": "^3.3.9",
